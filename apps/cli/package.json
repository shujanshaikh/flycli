{
  "name": "flycli",
  "version": "0.1.0",
  "description": "Your ai coding agent, that lives in your browser",
  "module": "src/index.ts",
  "type": "module",
  "private": false,
  "bin": {
    "flycli": "./src/cli.ts"
  },
  "files": [
    "src",
    "toolbar",
    "README.md",
    "GETTING_STARTED.md",
    "QUICK_REFERENCE.md",
    "CHANGELOG.md",
    "flycli.config.example.json"
  ],
  "keywords": [
    "nextjs",
    "ai",
    "development",
    "toolbar",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/flycli.git"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/cli.ts --outdir dist --target bun",
    "build:toolbar": "cd ../toolbar && bun run build",
    "prepublishOnly": "bun run build:toolbar"
  },
  "exports": {
    "./cli": "./src/ws-server.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.23",
    "@inquirer/prompts": "5.5.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@repo/agent": "*",
    "@types/express": "^5.0.3",
    "@types/ws": "^8.18.1",
    "agents": "^0.2.14",
    "ai": "^5.0.65",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  }
}
